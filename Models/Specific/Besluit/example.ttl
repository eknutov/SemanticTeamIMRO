# Examples of ro:Besluit (at the moment only complete for besluitvlakken (modelled as Planobjects))
# TODO: Once correct, split the file to one file for each of the three subclasses of ro:Besluit.

# Example of ro:ProvincialeVerordening

ex:PV1 a ro:ProvincialeVerordening, ro:Plan, ro:Besluit, void:Dataset;
  rdfs:label "provinciale verordering 1";
  dc:hasPart ex:PlanobjectPV1_A;
.

ex:PlanobjectPV1_A a ro:Planobject;
  void:inDataSet ex:PV1;
  dc:isPartOf ex:PV1;
  rdfs:label "Zuid-Limburg";
  dc:subject "demogafie";
  ro:tekst ex:TekstPV1_1;
  ro:illustratie ex:IllustratiePV1_1;
  ro:kaart ex:KaartPV1_1;
  ogc:geometry ex:Geom1;
  dc:hasPart ex:PlanobjectPV1_A1;
  dc:hasPart ex:PlanobjectPV1_A2;
.

ex:TekstPV1_1 a ro:Document, ro:Tekst;
  void:inDataSet ex:PV1;
  ro:identificatie "document 1";
  ro:teksttype ro_beg:Toelichting;
  ro:normadressant ro_beg:Gemeenteraad;
.

ex:IllustratiePV1_1 a ro:Document, ro:Illustratie;
  void:inDataSet ex:PV1;
  ro:identificatie "C:/images/plaatje1.png";
  ro:illustratietype ro_beg:Kaart;
  ro:legendanaam "Overzichtskaart";
.

ex:KaartPV1_1 a ro:Kaart;
  void:inDataSet ex:PV1;
  rdfs:label "De hoofdkaart";
  dc:identifier "1"^^xsd:integer;
  ro:symboolcode "12345";
.

ex:GeomPV1_1 a ogc:Geometry;
  void:inDataSet ex:PV1;
  ogc:asWKT "POLYGON((3.3080 50.7504,3.3079 53.5764,7.2275 53.5764,7.2275 50.7504,3.3080 50.7504))"^^ogc:wktLiteral;
  ro:idealisatie ro_beg:Indicatief;
.

ex:PlanobjectPV1_A1 a ro:Planobject;
  void:inDataSet ex:PV1;
  dc:isPartOf ex:PlanobjectPV1_A;
  rdfs:label "Gulpen";
  dc:subject "demogafie";
.

ex:PlanobjectPV1_A2 a ro:Planobject;
  void:inDataSet ex:PV1;
  dc:isPartOf ex:PlanobjectPV1_A;
  rdfs:label "Eijsden";
  dc:subject "demogafie";
.

