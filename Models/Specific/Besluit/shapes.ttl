# common prefixes
@prefix dc: <http://purl.org/dc/terms/> .
@prefix doco: <http://purl.org/spar/doco/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix gml: <http://www.opengis.net/ont/gml#> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix ogc: <http://www.opengis.net/ont/geosparql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# prefixes specific to Informatiehuis Ruimte
@prefix imro: <http://data.imro.com/def/> .
@prefix nen3610: <http://data.informatiehuisruimte.nl/def/nen3610#> .
@prefix nen3610_str: <http://data.informatiehuisruimte.nl/def/nen3610/> .
@prefix pdok_pdok: <http://data.pdok.nl/def/pdok#> .
@prefix ro: <http://data.informatiehuisruimte.nl/def/ro#> .
@prefix ro_beg: <http://data.informatiehuisruimte.nl/ro/id/begrip/> .
@prefix ro_begkr: <http://data.informatiehuisruimte.nl/id/begrippenkader/> .
@prefix ro_col: <http://data.informatiehuisruimte.nl/id/collectie/> .
@prefix ro_nen3610_beg: <http://data.informatiehuisruimte.nl/nen3610/id/begrip/> .
@prefix ro_str: <http://data.informatiehuisruimte.nl/def/ro/> .
@prefix ro_sym: <http://data.informatiehuisruimte.nl/ro/id/symbool/> .


ro_str:Besluit a sh:NodeShape ;
  sh:targetClass ro:Besluit ;
  sh:property
    ro_str:Besluit_spatial
.

ro_str:Besluit_spatial a sh:PropertyShape ;
  sh:path dc:spatial ;
  sh:node
    ro_str:Besluit_Plangebied_begrenzing
.

ro_str:Besluit_Plangebied_begrenzing a sh:PropertyShape ;
  sh:path ro:begrenzing ;
  sh:node 
    ro_str:VlakMultiVlak
.




# Provinciale verordening
ro_str:ProvincialeVerordening a sh:NodeShape;
  sh:targetClass ro:ProvincialeVerordening;
  sh:property [
    sh:path dc:hasPart;
    sh:node ro_str:ProvincialeVerordening_Planobject;
  ]
.

ro_str:ProvincialeVerordening_Planobject a sh:NodeShape;
  sh:property
    ro_str:ProvincialeVerordening_Plannobject_subject,
    ro_str:ProvincialeVerordening_Plannobject_tekst,
    ro_str:ProvincialeVerordening_Plannobject_illustratie,
    ro_str:ProvincialeVerordening_Plannobject_kaart,
    ro_str:ProvincialeVerordening_Plannobject_hasPart;
.

ro_str:ProvincialeVerordening_Plannobject_subject a sh:PropertyShape;
  sh:property [
    sh:path dc:subject;
    sh:nodeKind sh:Literal;
    sh:minCount 1;
  ];
.

ro_str:ProvincialeVerordening_Plannobject_tekst a sh:PropertyShape;
  sh:path ro:tekst;
  sh:class ro:Tekst;
  sh:node ro_str:ProvincialeVerordening_Plannobject_tekst_teksttype;
  sh:node ro_str:ProvincialeVerordening_Plannobject_tekst_normadressant;
.

ro_str:ProvincialeVerordening_Plannobject_tekst_teksttype a sh:PropertyShape;
  sh:path ro:teksttype;
  sh:class skos:Concept;
  sh:minCount 1;
  sh:maxCount 1;
  sh:node [
    sh:property [
       sh:path [ sh:inversePath skos:member ];
       sh:hasValue ro_col:TekstypesPlanobjectProvinciaalBesluit;
    ]
  ]
.

ro_str:ProvincialeVerordening_Plannobject_tekst_normadressant a sh:PropertyShape;
  sh:path ro:normadressant;
  sh:class skos:Concept;
  sh:node [
    sh:property [
       sh:path [ sh:inversePath skos:member ];
       sh:hasValue ro_col:NormadressantenProvincialeVerorderdening;
    ]
  ]
.

ro_str:ProvincialeVerordening_Plannobject_illustratie a sh:PropertyShape;
  sh:property [
    sh:path ro:illustratie;
    sh:nodeKind sh:IRI;
    sh:class ro:Illustratie;
  ];
.

ro_str:ProvincialeVerordening_Plannobject_kaart a sh:PropertyShape;
  sh:property [
    sh:path ro:kaart;
    sh:nodeKind sh:IRI;
    sh:class ro:Kaart;
  ];
.

ro_str:ProvincialeVerordening_Plannobject_hasPart a sh:PropertyShape;
  sh:property [
    sh:path dc:hasPart;
    sh:node ro_str:ProvincialeVerordening_Planobject;
  ];
.

