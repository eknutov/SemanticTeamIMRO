ro_str:OverigGebiedsgerichtBesluit_Plannobject_tekst_normadressant a sh:PropertyShape;
  sh:path ro:normadressant;
  sh:class skos:Concept;
  sh:node [
    sh:property [
       sh:path [ sh:inversePath skos:member ];
       sh:hasValue ro_col:NormadressantenOverigGebiedsgerichtBesluit;
    ]
  ]
.


ro_str:OverigGebiedsgerichtBesluit_Plannobject_hasPart a sh:PropertyShape;
  sh:property [
    sh:path dc:hasPart;
    sh:node ro_str:OverigGebiedsgerichtBesluit_Planobject;
  ];
.
