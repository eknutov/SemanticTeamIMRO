# Overige gebiedsgebonden besluiten
# + naam: ScopedName
# + verwijzingNaarTekstInfo: TekstReferentie_XGB [1..*]
# + geometrie: VlakMultiVlak

ro_str:OverigGebiedsgerichtBesluit a sh:NodeShape;
