ro_str:OverigGebiedsgerichtBesluit_Plannobject_tekst_teksttype a sh:PropertyShape;
  sh:path ro:teksttype;
  sh:class skos:Concept;
  sh:minCount 1;
  sh:maxCount 1;
  sh:node [
    sh:property [
       sh:path [ sh:inversePath skos:member ];
       sh:hasValue ro_col:TekstypesPlanobjectOverigGebiedsgerichtBesluit;
    ]
  ]
