#################################################################
#    Structuurvisieplangebied_P
#################################################################

ro_str:Structuurvisieplangebied_P rdf:type sh:NodeShape ;
  sh:targetClass ro:Structuurvisieplangebied_P ;
  sh:property
    ro_str:Structuurvisieplangebied_P_typePlan ,
    ro_str:Structuurvisieplangebied_P_beleidsmatigVerantwoordelijkeOverheid ,
    ro_str:Structuurvisieplangebied_P_illustratieverwijzing ,
    ro_str:Structuurvisieplangebied_P_tekstverwijzing ,
    ro_str:Structuurvisieplangebied_P_externPlan ,
    ro_str:Structuurvisieplangebied_P_verwijzingNorm ,
    ro_str:Structuurvisieplangebied_P_begrenzing ;
  sh:sparql [
      sh:message "At least one Structuurvisieplangebied_P should have {$this} as its ro:plangebied." ;
      sh:prefixes ro_str:prefixes ;
      sh:select """
SELECT $this
WHERE {
  $this rdf:type ro:Structuurvisieplangebied_P .
  FILTER NOT EXISTS {
    ?structuurvisiegebied ro:plangebied $this ;
           rdf:type ro:Structuurvisiegebied_P .
  }
}
""" ;
    ] ;
.

ro_str:Structuurvisieplangebied_P_typePlan #same as for structuurvisieplangebied_R, perhaps lose P_ and keep use the same sh:PropertyShape.
  rdf:type sh:PropertyShape ;
  sh:path ro:typePlan ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
  sh:node ro_str:RuimtelijkPlanOfBesluit_SV ;
.

ro_str:Structuurvisieplangebied_P_beleidsmatigVerantwoordelijkeOverheid
  rdf:type sh:PropertyShape ;
  sh:path ro:beleidsmatigerantwoordelijkeOverheid ;
  sh:minCount 1 ;
  sh:node ro_str:Overheden_P
.

ro_str:Structuurvisieplangebied_P_illustratieverwijzing
  rdf:type sh:PropertyShape ;
  sh:path ro:illustratie ;
  sh:class ro:Illustratie ;
  sh:node ro_str:Illustratie ;
.

ro_str:Structuurvisieplangebied_P_tekstverwijzing  #same as for structuurvisieplangebied_R, perhaps lose P_ and keep use the same sh:PropertyShape.
  rdf:type sh:PropertyShape ;
    sh:path ro:tekst ;
    sh:minCount 1 ;
    sh:maxCount 3 ;
    sh:node ro_str:TeksttypePG_SV ;
    sh:and (
      [
        sh:qualifiedValueShape [
          sh:path rdf:type  ;
          sh:hasValue ro:Document ;
        ] ;
        sh:qualifiedMinCount 1 ;
        sh:qualifiedMaxCount 1 ;
      ]
      [
        sh:qualifiedValueShape [
          sh:path  rdf:type  ;
          sh:hasValue ro:Bijlage ;
        ] ;
        sh:qualifiedMaxCount 1 ;
      ]
    ) ;
.

ro_str:Structuurvisieplangebied_P_externPlan rdf:type sh:PropertyShape ; #same as for structuurvisieplangebied_R, perhaps lose P_ and keep use the same sh:PropertyShape.
  sh:path ro:externPlan ;
  sh:maxCount 1 ;
  sh:node ro_str:ExternPlanReferentiePG_SV ;
.

ro_str:Structuurvisieplangebied_R_verwijzingNorm
  rdf:type sh:PropertyShape ;
  sh:path ro:verwijzingNorm ;
  sh:minCount 2 ;
  sh:maxCount 3 ;
  sh:and ( [
    sh:qualifiedValueShape [
      sh:hasValue ro_beg:IMRO2012 ;

      ] ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedMaxCount 1 ;
    ]
    [
    sh:qualifiedValueShape [
      sh:hasValue ro_beg:PRpSV2012 ;
    ] ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedMaxCount 1 ;
    ]
    [
    sh:qualifiedValueShape [
      sh:hasValue ro_beg:IMROPT2012 ;
    ] ;
    sh:qualifiedMaxCount 1 ;
  ]
  )
.

ro_str:Structuurvisieplangebied_P_begrenzing
  rdf:type sh:PropertyShape ;
  sh:path ro:begrenzing ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
  sh:class ro:Geometrie ;
  sh:node ro_str:Structuurvisieplangebied_P_Geometrie
.

ro_str:Structuurvisieplangebied_P_Geometrie rdf:type sh:NodeShape ;
  sh:property
    ro_str:Structuurvisieplangebied_P_Geometrie_geometrie ,
    ro_str:Structuurvisieplangebied_P_Geometrie_inwinningsschaal ,
    ro_str:Structuurvisieplangebied_P_Geometrie_idealisatie ;
.

ro_str:Structuurvisieplangebied_P_Geometrie_geometrie rdf:type sh:PropertyShape ; #same as for structuurvisieplangebied_R, perhaps lose P_ and keep use the same sh:PropertyShape.
  sh:path ro:geometrie ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
#  sh:node ro_str:VlakMultiVlak ; #existing vocabulary is to be reused.
.

ro_str:Structuurvisieplangebied_P_Geometrie_inwinningsschaal rdf:type sh:PropertyShape ;
  sh:path ro:inwinningsschaal ;
  sh:maxCount 1 ;
  sh:datatype xsd:integer ;
.

ro_str:Structuurvisieplangebied_P_Geometrie_idealisatie  rdf:type sh:PropertyShape ; #same as for structuurvisieplangebied_R, perhaps lose P_ and keep use the same sh:PropertyShape.
  sh:path ro:idealisatie ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
  sh:node ro_str:Idealisatie_1 ;
.


