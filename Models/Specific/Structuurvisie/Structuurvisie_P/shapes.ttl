@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ogc: <http://www.opengis.net/ont/geosparql#> .
@prefix pdok_pdok: <http://data.pdok.nl/def/pdok#> .
@prefix gml: <http://www.opengis.net/ont/gml#> .
@prefix nen3610_str: <http://data.informatiehuisruimte.nl/def/nen3610/> .
@prefix nen3610: <http://data.informatiehuisruimte.nl/def/nen3610#> .
@prefix ro_str: <http://data.informatiehuisruimte.nl/def/ro/> .
@prefix ro: <http://data.informatiehuisruimte.nl/def/ro#> .
@prefix ro_col: <http://data.informatiehuisruimte.nl/id/collectie/> .
@prefix dcterms: <http://purl.org/dc/terms/> .


#################################################################
#    Structuurvisie_P
#################################################################

ro_str:Structuurvisie_P a sh:NodeShape ;
  sh:targetClass ro:Structuurvisieplangebied_P ;
  sh:property
    ro_str:Structuurvisie_P_beleidsmatigVerantwoordelijkeOverheid ,
    ro_str:Structuurvisie_P_hasPart ,
    ro_str:Structuurvisie_P_spatial ;
  sh:sparql [
      sh:message "At least one Structuurvisie_P should have {$this} as its ro:plangebied." ;
      sh:prefixes ro_str:prefixes
      sh:select """
SELECT $this
WHERE {
  $this rdf:type ro:Structuurvisieplan_P .
  FILTER NOT EXISTS {
    ?structuurvisiegebied ro:plangebied $this ;
           rdf:type ro:Structuurvisiegebied_P .
  }
}
""" ;
    ] ;
.

ro_str:Structuurvisie_P_beleidsmatigVerantwoordelijkeOverheid
  a sh:PropertyShape ;
  sh:path ro:beleidsmatigVerantwoordelijkeOverheid ;
  sh:minCount 1 ;
  sh:node ro_str:Overheden_P
.

ro_str:Structuurvisie_P_hasPart a sh:PropertyShape ;
  sh:path ( dct:hasPart rdf:type ) ;
  sh:in (
    ro:Structuurvisiegebied_P
    ro:Structuurvisiecomplex_P
  ) ;
  sh:qualifiedValueShape [
    sh:hasValue ro:Structuurvisiegebied_P
  ] ;
  sh:qualifiedMinCount 1 ;
.

ro_str:Structuurvisie_P_spatial a sh:PropertyShape ;
  sh:path dct:spatial ;
  sh:node ro_str:Structuurvisie_P_Plangebied
.

ro_str:Structuurvisie_P_Plangebied a sh:NodeShape ;
  sh:property
    ro_str:Structuurvisie_P_Plangebied_inwinningsschaal
.

ro_str:Structuurvisie_P_Plangebied_inwinningsschaal a sh:PropertyShape ;
  sh:path ro:inwinningsschaal ;
  sh:maxCount 1 ;
  sh:datatype xsd:integer ;
.


#################################################################
#    Structuurvisiegebied_P
#################################################################


ro_str:Structuurvisiegebied_P rdf:type sh:NodeShape ;
    sh:targetClass ro:Structuurvisiegebied_P ;
    sh:property
      ro_str:Structuurvisiegebied_P_beleid ,
      ro_str:Structuurvisiegebied_P_tekstVerwijzing ,
      ro_str:Structuurvisiegebied_P_illustratieVerwijzing , #partially common
      ro_str:Structuurvisiegebied_P_plangebied , #isPartOf
      ro_str:Structuurvisiegebied_P_begrenzing
.

ro_str:Structuurvisiegebied_P_thema rdf:type sh:PropertyShape ; #same as for structuurvisieplangebied_R, perhaps lose P_ and keep use the same sh:PropertyShape.
  sh:path ro:thema ;
  sh:minCount 1 ;
  sh:datatype xsd:string ;
.

ro_str:Structuurvisiegebied_P_beleid rdf:type sh:PropertyShape ;
  sh:path ro:beleid ;
  sh:minCount 1 ;
  sh:node ro_str:BeleidInfo_PSV ;
.

ro_str:BeleidInfo_PSV rdf:type sh:NodeShape ; #treated as a blank node
  sh:property
    BeleidInfo_PSV_belang ,
    BeleidInfo_PSV_rol ,
    BeleidInfo_PSV_instrument ;
.

ro_str:BeleidInfo_PSV_belang rdf:type sh:PropertyShape ;
  sh:path ro:belang ;
  sh:datatype xsd:string ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
.

ro_str:BeleidInfo_PSV_rol rdf:type sh:PropertyShape ;
  sh:path ro:rol ;
  sh:datatype xsd:string ;
  sh:minCount 1 ;
  sh:maxCount 1 ;
.

ro_str:BeleidInfo_PSV_instrument rdf:type sh:PropertyShape ;
  sh:path ro:instrument ;
  sh:maxCount 1 ;
  sh:in ( #were instruments named individuals? # for now yes, TODO make skos:Concepts.
    ro:bestuurlijkeAfspraken
    ro:coordinatieregeling
    ro:inpassingsplan
    ro:omgevingsvergunning
    ro:proactieveAanwijzing
    ro:reactieveAanwijzing
    ro:verordening
    ro:vooroverleg
    ro:zienswijze
  )
.

ro_str:Structuurvisiegebied_P_tekstVerwijzing rdf:type sh:PropertyShape ;
  sh:path ro:tekst ;
  sh:minCount 1 ;
  sh:node ro_str:Teksttype_PSV ;
.

ro_str:Structuurvisiegebied_P_illustratieVerwijzing rdf:type sh:PropertyShape ;
  sh:path ro:illustratie ;
  sh:class ro:Illustratie ;
  sh:node ro_str:Illustratie_illustratieNaam
.

ro_str:Structuurvisiegebied_P_plangebied rdf:type sh:PropertyShape ;
  sh:path ro:plangebied ;
  sh:class ro:Structuurvisieplangebied_P ;
.

ro_str:Structuurvisiegebied_P_begrenzing rdf:type sh:PropertyShape ;
  sh:path ro:begrenzing ;
  sh:node ro_str:Structuurvisiegebied_P_Geometrie ;
.

#partially defined by ro_str:Structuurvisiegebied.
ro_str:Structuurvisiegebied_P_Geometrie rdf:type sh:NodeShape ;
  sh:property
    ro_str:Structuurvisiegebied_P_Geometrie_inwinningsschaal
.

ro_str:Structuurvisiegebied_P_Geometrie_inwinningsschaal rdf:type sh:PropertyShape ;
  sh:path ro:inwinningschaal ;
  sh:maxCount 1 ;
  sh:datatype xsd:integer ;
  #TODO: not same as plangebied_inwinningsschaal
.


#################################################################
#    Structuurvisiecomplex_P
#################################################################

ro_str:Structuurvisiecomplex_P rdf:type sh:NodeShape ;
  sh:targetClass ro:Structuurvisiecomplex_P ;
  sh:property
    ro_str:Structuurvisiecomplex_P_beleid ,
    ro_str:Structuurvisiegebied_P_tekstVerwijzing , #copied
    ro_str:Structuurvisiegebied_P_begrenzing , #covers the inwinningschaal part.
    ro_str:Structuurvisiecomplex_P_isPartOf # multiplicity is defined in the superclass shape.
.

ro_str:Structuurvisiecomplex_P_beleid rdf:type sh:PropertyShape ;
  sh:path ro:beleid ;
  sh:minCount 1 ;
  sh:node ro_str:BeleidInfo_PSV ;
.

ro_str:Structuurvisiecomplex_P_isPartOf a sh:PropertyShape ;
  sh:path dct:isPartOf ;
  sh:class ro:Structuurvisie_P ;
.


#################################################################
#    Structuurvisieverklaring_P
#################################################################

ro_str:Structuurvisieverklaring_P rdf:type sh:NodeShape ;
  sh:targetClass ro:Structuurvisieverklaring_P ;
  sh:property
    ro_str:Structuurvisiecomplex_thema , #copied
    ro_str:Structuurvisieverklaring_P_tekstVerwijzing ,
    ro_str:Structuurvisieverklaring_illustratieverwijzing ,
    ro_str:Structuurvisiegebied_cartografie #copied
.

ro_str:Structuurvisieverklaring_P_tekstVerwijzing a sh:PropertyShape ;
  sh:path ro:tekst ;
  sh:node ro_str:TeksttypeP_PSV ;

ro_str:TeksttypeP_PSV rdf:type sh:NodeShape ;
  sh:property [
    sh:path rdf:type
    sh:in (
      ro:Toelichting
    )
  ]
.

ro_str:Structuurvisieverklaring_illustratieverwijzing a sh:PropertyShape ;
  sh:path ro:illustratie ;
  sh:class ro:Illustratie ;
  sh:node
    ro_str:Illustratie ,
    ro_str:Illustratie_legendaNaam ,
    ro_str:Illustratie_illustratieNaam
.
