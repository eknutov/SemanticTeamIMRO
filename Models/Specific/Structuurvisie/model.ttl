#################################################################
#    Structuurvisie
#################################################################

ro:Structuurvisie a owl:Class ;
  rdfs:subClassOf ro:Plan ;
  rdfs:label "Structuurvisie"@nl ;
  rdfs:comment "Een plan met als doel de structuurvisie vast te leggen wat betrekking heeft op een bepaald gebied."@nl ;
  dcterms:subject ro_beg:Structuurvisie ;
  rdfs:isDefinedBy :ro
.


#################################################################
#    Structuurvisiegebied
#################################################################

ro:Structuurvisiegebied a owl:Class ;
  rdfs:isDefinedBy :ro ;
  rdfs:label "Structuurvisiegebied"@nl ;
  rdfs:comment "Een gebied waar waar een bepaald beleid op uitgevoerd wordt wat deel uitmaakt van een Structuurvisie."@nl ;
  dcterms:subject ro_beg:Structuurvisiegebied ;
  rdfs:subClassOf ro:Planobject
.


#################################################################
#    Structuurvisiecomplex
#################################################################

ro:Structuurvisiecomplex a owl:Class ;
  rdfs:isDefinedBy :ro ;
  rdfs:label "Structuurvisiegebied"@nl ;
  rdfs:comment "Een groep Planobjecten, bestaande uit Structuurvisiegebieden en Structuurvisiecomplexen."@nl ;
  dcterms:subject ro_beg:Structuurvisiecomplex ;
  rdfs:subClassOf ro:Planobject
.
