ro_str:Figuur a sh:NodeShape ;
  sh:targetClass ro:Figuur ;
  sh:property
    ro_str:Figuur_naam ,
    ro_str:Figuur_illustratie ,
    ro_str:Figuur_label ,
    ro_str:Figuur_tekstverwijzing ,
