:ro
  rdf:type owl:Ontology, voaf:Vocabulary ;
  dc:description "Dit is een vocabulaire dat gebruikt kan worden voor het beschrijven van ruimtelijke instrumenten (beleidsstukken die worden gebruikt voor ruimtelijke ordening). Dit vocabulaire is afgeleid van het Informatiemodel Ruimtelijke Ordening 2012 (IMRO2012)."@nl ;
  dc:title "Vocabulaire voor ruimtelijke ordening"@nl ;
  rdfs:label "Vocabulaire voor ruimtelijke ordening"@nl ;
  owl:imports <http://datashapes.org/dash> ;
  rdfs:seeAlso <http://ro-standaarden.geonovum.nl/2012/IMRO/1.2/IMRO2012-v1.2.pdf> ;
  sh:shapesGraph :ro_str ;
  voaf:extends :nen3610 ;
  dc:coverage dbp:Netherlands;
  dc:language "nl";
  dc:publisher dbp:Kadaster;
  owl:versionInfo "0.1.0";
  dc:created "2017-10-04"^^xsd:date;
  dc:modified "2017-10-06"^^xsd:date;
.

#################################################################
#    Plan*
#################################################################

ro:Plan a owl:Class ;
  rdfs:label "Plan"@nl ;
  rdfs:isDefinedBy :ro ;
  rdfs:comment "Een plan dat regels beschrijft die geldig zijn voor een bepaald gebied."@nl ;
  dcterms:subject ro_beg:Plan
.

ro:Plangebied a owl:Class ;
  rdfs:label "Plangebied"@nl ;
  rdfs:isDefinedBy :ro ;
  rdfs:comment "Het gebied waar een specifiek plan voor geldt."@nl ;
  dcterms:subject ro_beg:Plangebied ;
  rdfs:subClassOf nen3610:PlanologischGebied
.

ro:Planobject a owl:Class ;
  rdfs:label "Planobject"@nl ;
  rdfs:isDefinedBy :ro ;
  rdfs:comment "Ruimtelijk object dat zich bevindt in een bijbehorend plangebied."@nl ;
  dcterms:subject ro_beg:Planobject ;
  rdfs:subClassOf nen3610:PlanologischGebied
.

