#Jesse: Since IMROPT is an other standard than IMRO, it should have its own ontology entity?

:roPT
  a owl:Ontology ;
  dc:description "TODO"@nl ;
  dc:title "Ruimtelijke Ordening Planteksten vocabulaire"@nl ;
  rdfs:label "Ruimtelijke Ordening Planteksten vocabulaire"@nl ;
  owl:imports <http://datashapes.org/dash> ;
#  sh:shapesGraph :ro_str #<- TODO
.


#################################################################
#    TekstObject
#
# TODO: see DOCO ontology
#################################################################

ro:Tekstobject a owl:Class ;
  rdfs:label "Tekstobject"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:subClassOf doco:Section ;
  dcterms:subject ro_beg:Tekstobject
.

ro:identificatie a owl:DatatypeProperty ;
  rdfs:label "identificatie"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject
.

ro:plan a owl:ObjectProperty ;
  rdfs:label "plan"@nl ;
  rdfs:isDefinedby :roPT ;
  rdfs:range ro:Plan
.

ro:volgnummer a owl:DatatypeProperty ;
  rdfs:label "volgnummer"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject
.

ro:niveau a owl:DatatypeProperty ;
  rdfs:label "niveau"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject
.

ro:ouder a owl:ObjectProperty ;
  rdfs:label "ouder"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject ;
  rdfs:range ro:Tekstobject ;
  rdfs:subPropertyOf pattern:isContainedBy
.

ro:externeVerwijzing a owl:DatatypeProperty ;
  rdfs:label "externe verwijzing"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject ;
.

ro:interneVerwijzing a owl:ObjectProperty ;
  rdfs:label "interneVerwijzing"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject ;
  rdfs:range ro:Tekstobject ;
.

ro:tekstMetadata a owl:ObjectProperty ;
  rdfs:label "tekstMetadata"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject ;
  rdfs:range ro:TekstMetadata
.

# Jesse: ro:tekst has been used to point to instances of ro:Tekst. Therefore I renamed the following property to ro:plantekst.
ro:plantekst a owl:DatatypeProperty ;
  rdfs:label "plantekst"@nl ;
  rdfs:isDefinedBy :roPT ;
  rdfs:domain ro:Tekstobject ;
  rdfs:subPropertyOf c4o:hasContent ;
.


