#################################################################
#    Tekstobject
#################################################################

ropt_str:TekstObject a sh:NodeShape ;
  sh:targetClass ropt:Tekstobject ;
  sh:property
    ropt_str:TekstObject_verwijzingNaarPlan ,
    ropt_str:TekstObject_volgnummer ,
    ropt_str:TekstObject_niveau ,
    ropt_str:TekstObject_type ,
    ropt_str:TekstObject_typeTekst ,
    ropt_str:TekstObject_titel ,
    ropt_str:TekstObject_externeVerwijzing ,
    ropt_str:TekstObject_tekst ,
#    ropt_str:TekstObject_ouder ,
    ropt_str:TekstObject_naamPraktijkRichtlijn
.

ropt_str:TekstObject_verwijzingNaarPlan a sh:PropertyShape ;
  sh:path ropt:plan ;
  sh:class ro:Plan ;
  sh:nodeKind sh:IRI ;
  sh:minCount 1 ;
  sh:maxCount 1
.

ropt_str:TekstObject_volgnummer a sh:PropertyShape ;
  sh:path ropt:volgnummer ;
  sh:nodeKind sh:Literal ;
  sh:datatype xsd:integer ;
  sh:minCount 1 ;
  sh:maxCount 1
.

ropt_str:TekstObject_niveau a sh:PropertyShape ;
  sh:path ropt:niveau ;
  sh:nodeKind sh:Literal ;
  sh:datatype xsd:integer ;
  sh:minCount 1 ;
  sh:maxCount 1
.

#Jesse: The allowed Tekstobjects depend on the type of plan. The following enumeration are defined in IMROPT:
# - ObjecttypePlan,
# - ObjecttypeVisie of
# - ObjecttypeBesluit
# e.g. if respective plan is of type ro:Structuurvisie, pick TekstObject from enumeration ObjecttypeVisie.
# This is reflected in the following shape:

ropt_str:TekstObject_type a sh:PropertyShape ;
  sh:qualifiedValueShape [
    sh:xone (
      [ sh:and (
          [ sh:path ( ropt:plan rdf:type ) ; sh:class ro:Bestemmingsplan ]
          [ sh:path rdf:type ; sh:node ropt_str:ObjecttypePlan ] ) ]
      [ sh:and (
          [ sh:path ( ropt:plan rdf:type ) ; sh:class ro:Structuurvisie ]
          [ sh:path rdf:type ; sh:node ropt_str:ObjecttypeVisie ] ) ]
      [ sh:and (
          [ sh:path ( ropt:plan rdf:type ) ; sh:class ro:Besluit ]
          [ sh:path rdf:type ; sh:node ropt_str:ObjecttypeBesluit ] ) ]
    ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:qualiedMaxCount 1
.

#Jesse: Should this type not be assigned to the actual tekst? (pointed to by the predicate ropt:planTekst)
ropt_str:TekstObject_typeTekst a sh:PropertyShape ;
  sh:qualifiedValueShape [
    sh:path rdf:type ;
    sh:class ro:Tekst ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:qualiedMaxCount 1
.

ropt_str:TekstObject_tekst a sh:PropertyShape ;
  sh:path ropt:tekst ;
  sh:nodeKind sh:Literal ;
  sh:datatype rdf:XMLLiteral ;
  sh:maxCount 1
.

ropt_str:TekstObject_titel a sh:PropertyShape ;
  sh:path ropt:titel ;
  sh:class ropt:Titel ;
  sh:nodeKind sh:IRI ;
  sh:minCount 1 ;
  sh:maxCount 1
.

ropt_str:TekstObject_externeVerwijzing a sh:PropertyShape ;
  sh:path ropt:externeVerwijzing ;
  sh:nodeKind sh:Literal ;
  sh:datatype xsd:string
.

# TODO: This seems to be part of the metadata of the document. Possibly move this to a higher level?
# Jesse: We could constrain it to only be an attibute of node with no ropt:ouderID (which I think is always of type ropt:Document?)

ropt_str:TekstObject_naamPraktijkRichtlijn a sh:PropertyShape ;
  sh:path ropt:naamPraktijkRichtlijn ;
  sh:class skos:Concept ;
  sh:nodeKind sh:IRI ;
  sh:maxCount 1 ;
  sh:node [
    sh:property [
      sh:path [ sh:inversePath skos:member ] ;
      sh:hasvalue ro_col:PraktijkRichtlijnNaam
    ]
  ]
.

