#################################################################
#    Tekstobject
#################################################################

ro_str:Tekstobject a sh:NodeShape ;
  sh:targetClass ro:Tekstobject ;
  sh:property
#    TODO: Is this actually necessary?
    ro_str:Tekstobject_verwijzingNaarPlan ,
#    TODO: Is this actually necessary?
    ro_str:Tekstobject_volgnummer ,
#    TODO: Is this actually necessary?
    ro_str:Tekstobject_niveau ,
#    TODO: Is this actually necessary?
    ro_str:TekstObject_type ,
    ro_str:Tekstobject_typeTekst ,
    ro_str:Tekstobject_titel ,
    ro_str:Tekstobject_externeVerwijzing ,
    ro_str:Tekstobject_tekst ,
    #ro_str:Tekstobject_ouder ,
    ro_str:Tekstobject_naamPraktijkRichtlijn
.

ro_str:Tekstobject_verwijzingNaarPlan a sh:PropertyShape ;
  sh:path ro:plan ;
  sh:class ro:Plan ;
  sh:nodeKind sh:IRI ;
  sh:minCount 1 ;
  sh:maxCount 1
.

ro_str:Tekstobject_volgnummer a sh:PropertyShape ;
  sh:path ro:volgnummer ;
  sh:nodeKind sh:Literal ;
  sh:datatype xsd:integer ;
  sh:minCount 1 ;
  sh:maxCount 1
.

ro_str:Tekstobject_niveau a sh:PropertyShape ;
  sh:path ro:niveau ;
  sh:nodeKind sh:Literal ;
  sh:datatype xsd:integer ;
  sh:minCount 1 ;
  sh:maxCount 1
.

#Jesse: The allowed Tekstobjects depend on the type of plan. The following enumeration are defined in IMROPT:
# - ObjecttypePlan,
# - ObjecttypeVisie of
# - ObjecttypeBesluit
# e.g. if respective plan is of type ro:Structuurvisie, pick TekstObject from enumeration ObjecttypeVisie.
# This is reflected in the following shape:

ro_str:Tekstobject_type a sh:PropertyShape ;
  sh:qualifiedValueShape [
    sh:xone (
      [ sh:and (
          [ sh:path ( ro:plan rdf:type ) ; sh:class ro:Bestemmingsplan ]
          [ sh:path rdf:type ; sh:node ro_str:ObjecttypePlan ] ) ]
      [ sh:and (
          [ sh:path ( ro:plan rdf:type ) ; sh:class ro:Structuurvisie ]
          [ sh:path rdf:type ; sh:node ro_str:ObjecttypeVisie ] ) ]
      [ sh:and (
          [ sh:path ( ro:plan rdf:type ) ; sh:class ro:Besluit ]
          [ sh:path rdf:type ; sh:node ro_str:ObjecttypeBesluit ] ) ]
    ) ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:qualiedMaxCount 1
.

#Jesse: Should this type not be assigned to the actual tekst? (pointed to by the predicate ro:planTekst)
ro_str:Tekstobject_typeTekst a sh:PropertyShape ;
  sh:qualifiedValueShape [
    sh:path rdf:type ;
    sh:class ro:Tekst ;
    sh:nodeKind sh:IRI ;
  ] ;
  sh:qualiedMaxCount 1
.

ro_str:Tekstobject_titel a sh:PropertyShape ;
  sh:path ro:titel ;
  sh:class ro:Titel ;
  sh:nodeKind sh:IRI ;
  sh:minCount 1 ;
  sh:maxCount 1
.

ro_str:Tekstobject_externeVerwijzing a sh:PropertyShape ;
  sh:path ro:externeVerwijzing ;
  sh:nodeKind sh:Literal ;
  sh:datatype xsd:string
.

ro_str:Tekstobject_tekst a sh:PropertyShape ;
  sh:path ro:plantekst ;
  sh:nodeKind sh:Literal ;
  sh:datatype rdf:XMLLiteral ;
  sh:maxCount 1
.

# TODO: This seems to be part of the metadata of the document. Possibly move this to a higher level?
# Jesse: We could constrain it to only be an attibute of node with no ro:ouderID (which I think is always of type ro:Document?)

ro_str:Tekstobject_naamPraktijkRichtlijn a sh:PropertyShape ;
  sh:path ro:naamPraktijkRichtlijn ;
  sh:class skos:Concept ;
  sh:nodeKind sh:IRI ;
  sh:maxCount 1 ;
  sh:node [
    sh:property [
      sh:path [ sh:inversePath skos:member ] ;
      sh:hasvalue ro_col:PraktijkRichtlijnNaam
    ]
  ]
.

